{
    "dependencies": {
        "@prisma/client": "2.17.0",
        "bcrypt": "^5.0.1",
        "express": "^4.17.1"
    },
    "scripts": {
        "start": "dotenv -e .env.production -- node index.js",
        "dev": "dotenv -e .env -- nodemon index.js",
        "db:up": "dotenv -e .env -- prisma db push --preview-feature --accept-data-loss --force-reset && prisma generate && yarn db:seed",
        "db:seed": "node prisma/seed.js",
        "test": "dotenv -e .env.test.local -- jest --verbose --runInBand"
    },
    "devDependencies": {
        "dotenv": "^8.2.0",
        "faker": "^5.4.0",
        "jest": "^26.6.3",
        "nodemon": "^2.0.7",
        "prisma": "2.17.0",
        "supertest": "^6.1.3"
    }
}
